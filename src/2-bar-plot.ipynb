   "source": [
    "import matplotlib.pyplot as plt\n",
    "categories = ['A', 'B', 'C', 'D']\n",
    "values = [5, 7, 3, 9]\n",
    "colors = ['Red' , '#FF4500', '#4682B4', '#8B008B']\n",
    "plt.bar(categories, values, color=colors)\n",
    "plt.title(\"Bar Plot of Categories vs Values\")\n",
    "plt.show()\n"
   ]
